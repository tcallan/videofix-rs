default_target: "sony"
targets: [
    name: "sony"
    format_spec:
        audio:
            Reject: ["eac3", "vorbis", "opus"]
        video:
            Reject: ["vp9"]
        container:
            Reject: [],
    name: "roku"
    format_spec:
        audio:
            Allow: ["aac", "ac3", "eac3", "mp3", "flac", "dts"]
        video:
            Allow: ["h264", "hevc"]
        container:
            Allow: ["matroska", "mov"]
]